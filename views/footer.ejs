<script type="text/javascript">
  var lastScrollTop = 0;
  $(window).scroll(function(event)
  {
   var st = $(this).scrollTop();
   if (st > lastScrollTop)
   {
      $(".smart_scroll").slideUp();
      $(".arrow_up").css("display", "block");
   }
   else
   {
      $(".smart_scroll").slideDown();
      $(".arrow_up").css("display", "none");
   }
   lastScrollTop = st;
  });

</script>


